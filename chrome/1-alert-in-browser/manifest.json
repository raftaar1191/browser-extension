{
  "manifest_version": 2,
  "name": "Alert in Browser",
  "description": "Print an alert in browser when page load.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Alert in Browser"
  },

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["script.js"]
    }
  ]
}